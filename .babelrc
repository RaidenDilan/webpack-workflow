{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions"
        ]
      },
      "useBuiltIns": "usage",
      "corejs": {
        "version": 3,
        "proposals": true
      },
      "debug": false
    }],
    "@babel/preset-stage-2",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
